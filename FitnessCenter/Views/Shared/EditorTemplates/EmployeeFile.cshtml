@model FitnessCenter.Entities.EmployeeFile


<div class="col-md-offset-0 col-md-4" id="@Model.FileName">
    @Html.HiddenFor(model => model.FileName)
    <img class="thumbnail img-responsive" src="@Model.Path2"/>
@using (Html.BeginForm("DeleteFile", "Employee", FormMethod.Post, new { file = @Model.FileName }))
        {
    @Html.HiddenFor(model => model.FileName)
    @Html.HiddenFor(model => model.Employee.ID)
    <button type="submit" class="btn btn-default">
        <span class="glyphicon glyphicon-remove">
        </span>
    </button>
}
</div>