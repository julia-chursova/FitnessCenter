@model FitnessCenter.Entities.RoomFile

@using (Html.BeginForm("DeleteFile", "Room", FormMethod.Post, new { file = @Model.FileName }))
{
    <div class="col-md-offset-0 col-md-4" id="@Model.FileName">
        @Html.HiddenFor(model => model.FileName)
        <img class="thumbnail img-responsive" src="@Model.Path2"/>
        @Html.HiddenFor(model => model.Room.ID)
        <button type="submit" class="btn btn-default">
            <span class="glyphicon glyphicon-remove">
            </span>
        </button>

    </div>
}